{
	"name": "new-trip-app",
	"version": "1.0.0",
	"main": "index.ts",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"expo": "~54.0.33",
		"expo-status-bar": "~3.0.9",
		"react": "19.1.0",
		"react-native": "0.81.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.15",
		"@types/react": "~19.1.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"typescript": "~5.9.2"
	},
	"private": true
}

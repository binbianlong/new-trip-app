{
	"expo": {
		"name": "new-trip-app",
		"slug": "new-trip-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"scheme": "new-trip-app",
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#CAFACD"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.anonymous.new-trip-app"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#CAFACD"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"permissions": [
				"android.permission.RECORD_AUDIO",
				"android.permission.ACCESS_COARSE_LOCATION",
				"android.permission.ACCESS_FINE_LOCATION"
			]
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"cameraPermission": "旅行の写真を撮影するためにカメラを使用します"
				}
			],
			[
				"expo-location",
				{
					"locationWhenInUsePermission": "撮影場所を記録するために位置情報を使用します"
				}
			],
			[
				"expo-notifications",
				{
					"sounds": []
				}
			]
		],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "301d9c47-79d4-4dd0-b2ae-d31c929f9c2f"
			}
		},
		"owner": "t161121t"
	}
}
